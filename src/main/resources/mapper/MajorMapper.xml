<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="chnytrcy.xyz.campusepidemicsystem.mapper.MajorMapper">

  <select id="getMajorListByDept"
          resultType="chnytrcy.xyz.campusepidemicsystem.model.vo.pc.major.MajorListVO">
    SELECT
        name AS label,
        code AS code
    FROM
        major
    WHERE
        is_delete = 0
  </select>
</mapper>