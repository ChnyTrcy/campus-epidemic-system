<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="chnytrcy.xyz.campusepidemicsystem.mapper.ClassMapper">

  <select id="getClassListByMajorCode"
          resultType="chnytrcy.xyz.campusepidemicsystem.model.vo.pc.classvo.ClassListVO">
    SELECT
        name AS label,
        code AS code
    FROM
        class
    WHERE
        is_delete = 0
  </select>
</mapper>